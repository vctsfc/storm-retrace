<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <!-- Allow JIT compilation (Web Workers, WebAssembly) -->
  <key>com.apple.security.cs.allow-jit</key>
  <true/>
  <!-- Allow unsigned executable memory (V8 engine) -->
  <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
  <true/>
  <!-- Allow outbound network connections (S3, IEM, tile servers) -->
  <key>com.apple.security.network.client</key>
  <true/>
  <!-- Allow read-only file access for GPX import and exports -->
  <key>com.apple.security.files.user-selected.read-only</key>
  <true/>
  <!-- Allow read-write file access for downloads/exports -->
  <key>com.apple.security.files.user-selected.read-write</key>
  <true/>
</dict>
</plist>
